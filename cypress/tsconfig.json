{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "isolatedModules": false,
    "types": [
      "cypress",
      "@testing-library/cypress",
    ]
  },
"include": [
  "support/**/*.ts",
  "e2e/**/*.ts",
  "**/*.ts",
  "**/*.tsx",
  "cypress",
  "cypress/**/*.ts",
  "cypress/**/*.tsx",
  "cypress/**/*.js"
],
  "exclude": [
    "../node_modules",
    "../dist"
  ]
}